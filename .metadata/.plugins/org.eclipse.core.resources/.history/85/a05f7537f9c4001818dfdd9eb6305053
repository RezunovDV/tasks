package algorithms;

public class CamelCase {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String str = "saveChangesInTheEditor";
		
		String [] word = str.split("[ ]");
		
		for (int i = 0; i < word.length; i++) {
			System.out.println(word[i]);
		}
		
		
		
	}
}
